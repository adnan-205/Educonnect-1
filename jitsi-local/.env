# Jitsi Meet Local Development Configuration
# ⚠️  WARNING: This configuration is for LOCAL TESTING ONLY - NOT for production use!

# Basic Configuration
PUBLIC_URL=http://localhost
ENABLE_LETSENCRYPT=0
DISABLE_HTTPS=1
TZ=UTC

# Security Configuration (Generate new passwords for production!)
JICOFO_COMPONENT_SECRET=jicofo-secret-local-dev
JICOFO_AUTH_PASSWORD=jicofo-auth-password-local-dev
JVB_AUTH_PASSWORD=jvb-auth-password-local-dev
JIGASI_XMPP_PASSWORD=jigasi-xmpp-password-local-dev
JIBRI_RECORDER_PASSWORD=jibri-recorder-password-local-dev
JIBRI_XMPP_PASSWORD=jibri-xmpp-password-local-dev

# XMPP Configuration
XMPP_DOMAIN=meet.jitsi
XMPP_AUTH_DOMAIN=auth.meet.jitsi
XMPP_BOSH_URL_BASE=http://xmpp.meet.jitsi:5280
XMPP_SERVER=xmpp.meet.jitsi
XMPP_MUC_DOMAIN=muc.meet.jitsi
XMPP_INTERNAL_MUC_DOMAIN=internal-muc.meet.jitsi
XMPP_GUEST_DOMAIN=guest.meet.jitsi
XMPP_RECORDER_DOMAIN=recorder.meet.jitsi

# JVB Configuration
JVB_BREWERY_MUC=jvbbrewery
JVB_ENABLE_APIS=rest,colibri
JVB_STUN_SERVERS=meet-jit-si-turnrelay.jitsi.net:443

# Web Configuration
ENABLE_AUTH=0
ENABLE_GUESTS=1
ENABLE_LOBBY=1
ENABLE_HTTP_REDIRECT=0
ENABLE_HSTS=0

# Additional Features (Optional)
ENABLE_RECORDING=0
ENABLE_TRANSCRIPTIONS=0
ENABLE_OCTO=0

# Docker Network
DOCKER_HOST_ADDRESS=localhost
