  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc --noCheck",
    "build:prod": "npm run clean && tsc --build",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prod": "npm run build:prod && npm start",
    "docker:build": "docker build -t educonnect-backend .",
    "docker:run": "docker run -p 5000:5000 educonnect-backend",
    "render:build": "npm ci && npm run build",
    "render:start": "npm start"
    
  },
